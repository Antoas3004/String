public class Decryption {

	public static void main(String[] args) {

		String a = "mdyd";
		String b = "";
		char c[] = a.toCharArray();
		for (int i = 0; i < c.length; i = i + 1) {
			c[i] = (char) (c[i] - 3);
		}
		for (int i = 0; i < c.length; i = i + 1) {
			b = b + c[i];
		}
		System.out.println(b);

	}

}
