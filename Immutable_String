public class Immutable_String {

	public static void main(String[] args) {

		String S1 = "sachin";

		S1.concat("tendulkar"); // concat method appends the string at the end

		System.out.println(S1);

		// its only print String beacuse String in immutable

		S1 = S1.concat("tendular");

		System.out.println(S1);

		// if we Assign a value s1 then it will refer
	}
}
